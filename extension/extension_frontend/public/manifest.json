{
    "manifest_version": 3,
    "name": "SocialME",
    "description":"This is assosicated with SocialME app is your feed manager/tracker",
    "version": "1.0.0",
    "action":{
        "default_popup":"index.html"
    },
    "permissions":[
        "scripting",
        "tabs",
        "activeTab",
        "storage"
    ],
    "host_permissions":[
        "http://*/*",
        "https://*/*"
    ]
}