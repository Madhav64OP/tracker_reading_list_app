{
    "manifest_version": 3,
    "name": "SocialME",
    "description":"This is assosicated with SocialME app is your feed manager/tracker",
    "version": "1.0.0",
    "action":{
        "default_popup":"index.html"
    },
    "background":{
        "service_worker":"background.js",
        "type":"module"
    },
    "icons": {
        "16": "socialME.png",
        "32": "socialME.png",
        "48": "socialME.png",
        "128": "socialME.png"
    },
    "permissions":[
        "scripting",
        "tabs", 
        "activeTab",
        "storage",
        "webNavigation"
    ],
    "host_permissions":[
        "http://*/*",
        "https://*/*"
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
    },
    "web_accessible_resources":[
        {
            "resources": ["*.wasm", "*.params", "*.json", "*.metadata"],
            "matches": ["<all_urls>"]
        }
    ]
}